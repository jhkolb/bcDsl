contract UnsignedInt {
    data {
      foo: Uint
      bar: Int
    }

    initialize: ->(initValue: Uint) configured {
        foo = 3
        bar = -7
    }

    payOut: configured -> paid
    requires [ bar > -1 ] {
        foo = foo - 1
        bar = bar + -3
        send foo to sender consuming foo
    }
}
